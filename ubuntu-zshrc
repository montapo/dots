alias zshrc="vi ~/.zshrc"
alias srczshrc="source ~/.zshrc"

# vim
alias esnip="cd ~/.vim/snippets/my"
alias snip="cd ~/.vim/bundle/neosnippet/autoload/neosnippet/snippets"
